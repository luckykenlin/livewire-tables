{
    "name": "luckykenlin/livewire-tables",
    "description": "A dynamic table component for Laravel Livewire.",
    "keywords": [
        "luckykenlin",
        "livewire",
        "datatables",
        "tables",
        "laravel"
    ],
    "homepage": "https://github.com/luckykenlin/livewire-tables",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ken Lin",
            "email": "luckykenlin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "livewire/livewire": "^2.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Luckykenlin\\LivewireTables\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luckykenlin\\LivewireTables\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luckykenlin\\LivewireTables\\Providers\\LivewireTableServiceProvider"
            ]
        }
    }
}
